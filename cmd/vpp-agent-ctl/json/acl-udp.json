{
    "acl_name": "acl1",
    "interfaces": {
        "egress": [
            "tap1",
            "tap2"
        ],
        "ingress": [
            "tap3",
            "tap4"
        ]
    },
    "rules": [
        {
            "actions": {
                "acl_action": 1
            },
            "matches": {
                "ip_rule": {
                    "ip": {
                        "destination_network": "",
                        "source_network": "192.168.1.2/32"
                    },
                    "udp": {
                        "destination_port_range": {
                            "lower_port": 1150,
                            "upper_port": 1250
                        },
                        "source_port_range": {
                            "lower_port": 150,
                            "upper_port": 250
                        }
                    }
                }
            }
        }
    ]
}
