{
    "name": "${acl_name}",
    "interfaces": {
        "egress": [
            "${egr_intf1}"
        ],
        "ingress": [
            "${ingr_intf1}"
        ]
    },
    "rules": [
        {
            "action": ${acl_action},
            "ip_rule": {
                "ip": {
                    "destination_network": "${dest_ntw}",
                    "source_network": "${src_ntw}"
                },
                "tcp": {
                    "destination_port_range": {
                        "lower_port": ${dest_port_low},
                        "upper_port": ${dest_port_up}
                    },
                    "source_port_range": {
                        "lower_port": ${src_port_low},
                        "upper_port": ${src_port_up}
                    },
                    "tcp_flags_mask": ${tcp_flags_mask},
                    "tcp_flags_value": ${tcp_flags_value}
                }
            }
        }
    ]
}