{
    "acl_name": "${acl_name}",
    "interfaces": {
        "egress": [
            "${egr_intf1}"
        ],
        "ingress": [
            "${ingr_intf1}"
        ]
    },
    "rules": [
        {
            "rule_name":"${rule_nm}",
            "actions": {
                "acl_action": ${acl_action}
            },
            "matches": {
                "ip_rule": {
                    "ip": {
                        "destination_network": "${dest_ntw}",
                        "source_network": "${src_ntw}"
                    },
                    "tcp": {
                        "destination_port_range": {
                            "lower_port": ${dest_port_low},
                            "upper_port": ${dest_port_up}
                        },
                        "source_port_range": {
                            "lower_port": ${src_port_low},
                            "upper_port": ${src_port_up}
                        },
                        "tcp_flags_mask": 20,
                        "tcp_flags_value": 10
                    }
                }
            }
        }
    ]
}